<% semantic_form_for @app do |f| %>
	<%= f.input :title %>
	<%= f.input :author %>
	<%= f.input :demo, :label => "Link to Demo" %>
	<%= f.input :source, :label => "Link to Source" %>
	<%= f.input :description, :as => :text %>
	<%= f.input :samplecode, :label => "Sample Code", :as => :text %>
	<%= f.submit "Submit" %>
<% end %>